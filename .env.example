# ConvertKit API Configuration
# Get your API key from: https://app.convertkit.com/account_settings/advanced_settings
CONVERTKIT_API_KEY=your_convertkit_api_key_here

# Get your Form ID from your ConvertKit form URL
# Example: If your form URL is https://app.convertkit.com/forms/designers/12345
# Then your FORM_ID is 12345
CONVERTKIT_FORM_ID=your_form_id_here

# PostHog Analytics
# Get your API key from: https://app.posthog.com/project/settings
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_api_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://e.bartcagara.com

# RB2B Tracking
# Get your tracking key from RB2B dashboard
NEXT_PUBLIC_RB2B_KEY=your_rb2b_key_here
