# Copy this file to .env.local and fill in your values
# This file is already in .gitignore and won't be committed
# IMPORTANT: Never commit actual API keys to version control

# PostHog Analytics
# Get your key from: https://app.posthog.com/project/settings
NEXT_PUBLIC_POSTHOG_KEY=phc_YOUR_POSTHOG_KEY_HERE
NEXT_PUBLIC_POSTHOG_HOST=https://e.yourdomain.com

# RB2B Tracking
# Get your key from your RB2B dashboard
NEXT_PUBLIC_RB2B_KEY=YOUR_RB2B_KEY_HERE
